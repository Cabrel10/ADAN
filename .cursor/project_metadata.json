{
  "name": "adan-trading-bot",
  "version": "1.0.0",
  "description": "Système de trading algorithmique avancé utilisant l'apprentissage par renforcement",
  "main": "src/adan_trading_bot/__init__.py",
  "scripts": {
    "start": "python -m src.adan_trading_bot.main",
    "test": "pytest tests/ -v",
    "lint": "flake8 src/ tests/ && mypy src/ tests/",
    "format": "black src/ tests/ && isort src/ tests/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/votre-utilisateur/adan-trading-bot.git"
  },
  "keywords": [
    "trading",
    "algorithmic",
    "reinforcement-learning",
    "cryptocurrency",
    "finance"
  ],
  "author": "Votre Nom <votre.email@example.com>",
  "license": "MIT",
  "private": true,
  "engines": {
    "python": ">=3.9.0"
  },
  "cursor": {
    "projectType": "python",
    "features": [
      "debugging",
      "testing",
      "linting",
      "formatting",
      "intellisense"
    ],
    "linting": {
      "enabled": true,
      "tools": ["flake8", "mypy", "black", "isort"]
    },
    "testing": {
      "enabled": true,
      "framework": "pytest",
      "coverage": {
        "enabled": true,
        "reporters": ["html", "term"]
      }
    },
    "autoFix": {
      "onSave": true,
      "rules": [
        "autoFormat",
        "organizeImports",
        "fixSimpleErrors"
      ]
    },
    "intellisense": {
      "extraPaths": ["src"],
      "languageServer": "pylance"
    }
  }
}
